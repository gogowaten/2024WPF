<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

  <ControlTemplate TargetType="Thumb" x:Key="tb">
    <TextBlock Text="textblockthumb"/>
  </ControlTemplate>
  
  <ControlTemplate TargetType="Thumb" x:Key="rect">
    <Rectangle Fill="Orange"/>
  </ControlTemplate>

  <ControlTemplate TargetType="Thumb" x:Key="elli">
    <Ellipse Fill="MediumBlue"/>
  </ControlTemplate>
  
</ResourceDictionary>